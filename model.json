{

    "dimensions": [
        {
            "name": "date_base",
            "label": "Date Base",
            "levels": [
                {
                    "name":"year",
                    "label":"Year"
                },
                {
                    "name":"month",
                    "label":"Month"
                },
                {
                    "name":"day",
                    "label":"Day"
                },
                {
                    "name":"hours",
                    "label":"Hours"
                }
            ]
        },
        {
            "name": "date_weather",
            "label": "Weather Date",
            "template": "date_base"
        },
        {
            "name": "station",
            "label": "Station",
            "levels": [ {
                "name": "station",
                "label": "Station",
                "attributes": [ "id", "identifier", "number","name", "latitude", "longitude", "elevation"]
            } ]
        }
    ],
    "cubes": [
        {
            "name": "weather",
            "dimensions": ["date_weather", "station" ],
            "measures": [
                {"name": "PRES"},{"name": "HGHT"},{"name": "TEMP"},{"name": "DWPT"},{"name": "RELH"},{"name": "MIXR"},{"name": "DRCT"},{"name": "SKNT"},{"name": "THTA"},{"name": "THTE"},{"name": "THTV"}
            ],

            "joins": [
                {
                    "master": "weather.date_id",
                    "detail": "dates.id"
                },
                {
                    "master": "weather.station_id",
                    "detail": "stations.id"
                }
            ],
            "aggregates": [
                {
                    "name": "Average(TEMP)",
                    "function": "avg",
                    "measure": "TEMP"
                },
                {
                    "name": "Maximum(TEMP)",
                    "function": "max",
                    "measure": "TEMP"
                },
                {
                    "name": "Minimum(TEMP)",
                    "function": "min",
                    "measure": "TEMP"
                },
                {
                    "name": "Average(DWPT)",
                    "function": "avg",
                    "measure": "DWPT"
                },
                {
                    "name": "Maximum(DWPT)",
                    "function": "max",
                    "measure": "DWPT"
                },
                {
                    "name": "Minimum(DWPT)",
                    "function": "min",
                    "measure": "DWPT"
                },
                {
                    "name": "Average(RELH)",
                    "function": "avg",
                    "measure": "RELH"
                },
                {
                    "name": "Maximum(RELH)",
                    "function": "max",
                    "measure": "RELH"
                },
                {
                    "name": "Minimum(RELH)",
                    "function": "min",
                    "measure": "RELH"
                },
                {
                    "name": "Average(MIXR)",
                    "function": "avg",
                    "measure": "MIXR"
                },
                {
                    "name": "Maximum(MIXR)",
                    "function": "max",
                    "measure": "MIXR"
                },
                {
                    "name": "Minimum(MIXR)",
                    "function": "min",
                    "measure": "MIXR"
                },
                {
                    "name": "Average(DRCT)",
                    "function": "avg",
                    "measure": "DRCT"
                },
                {
                    "name": "Maximum(DRCT)",
                    "function": "max",
                    "measure": "DRCT"
                },
                {
                    "name": "Minimum(DRCT)",
                    "function": "min",
                    "measure": "DRCT"
                },
                {
                    "name": "Average(SKNT)",
                    "function": "avg",
                    "measure": "SKNT"
                },
                {
                    "name": "Maximum(SKNT)",
                    "function": "max",
                    "measure": "SKNT"
                },
                {
                    "name": "Minimum(SKNT)",
                    "function": "min",
                    "measure": "SKNT"
                },
                {
                    "name": "record_count",
                    "function": "count"
                }
            ],
            "mappings": {
                "date_weather.year": "dates.date_year",
                "date_weather.hours": "dates.date_hours",
                "date_weather.month": "dates.date_month",
                "date_weather.day": "dates.date_day",

                "station.id": "stations.id",
                "station.name": "stations.station_name",
                "station.number": "stations.station_number",
                "station.identifier": "stations.station_identifier",
                "station.latitude": "stations.station_latitude",
                "station.longitude": "stations.station_longitude",
                "station.elevation": "stations.station_elevation"
            }
        }
    ]
}
